<?xml version="1.0" encoding="UTF-8"?>
<Installer>
    <Name>@APP_NAME@</Name>
    <Version>@APP_VERSION_STRING@</Version>
    <Title>@APP_NAME@</Title>
    <Publisher>@APP_ORGANISATION@</Publisher>
    <ProductUrl>@APP_WEBSITE@</ProductUrl>
    <StartMenuDir>@APP_NAME@</StartMenuDir>

    <TargetDir>@APP_TARGET_DIR@</TargetDir>
    <AdminTargetDir>@APP_ADMIN_TARGET_DIR@</AdminTargetDir>

    <InstallerWindowIcon>installericon</InstallerWindowIcon>
    <InstallerApplicationIcon>ClinifaceAppW</InstallerApplicationIcon>
    <Watermark>watermark.png</Watermark>
    <WizardStyle>Modern</WizardStyle>
    <!--
    <WizardDefaultHeight>400</WizardDefaultHeight>
    <WizardDefaultWidth>600</WizardDefaultWidth>
    -->

    <MaintenanceToolName>@APP_NAME@Maintenance</MaintenanceToolName>
    <RunProgram>@APP_RUN_PROGRAM@</RunProgram>

